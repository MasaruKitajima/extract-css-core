{
  "name": "extract-css-core",
  "description": "Extract all CSS from a given url, both server side and client side rendered.",
  "version": "0.2.0",
  "homepage": "https://www.projectwallace.com/oss",
  "repository": "https://github.com/bartveneman/extract-css-core",
  "issues": "https://github.com/bartveneman/extract-css-core/issues",
  "license": "MIT",
  "author": "Bart Veneman",
  "keywords": [
    "extract",
    "css",
    "scrape",
    "get-css"
  ],
  "scripts": {
    "test": "xo && ava test"
  },
  "files": [
    "src"
  ],
  "main": "src/index.js",
  "engines": {
    "node": ">=8.0"
  },
  "xo": {
    "prettier": true,
    "rules": {
      "ava/no-skip-test": "warn"
    }
  },
  "prettier": {
    "useTabs": true,
    "semi": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "proseWrap": "always",
    "overrides": [
      {
        "files": "*.json",
        "options": {
          "useTabs": false
        }
      }
    ]
  },
  "devDependencies": {
    "ava": "^1.3.1",
    "create-test-server": "^2.4.0",
    "prettier": "^1.16.4",
    "xo": "^0.24.0"
  },
  "dependencies": {
    "puppeteer": "^1.13.0"
  }
}
